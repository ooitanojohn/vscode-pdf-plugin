[
  // TextTabels 初期値
  // Ctrl + q 後 Ctrl+q-テーブルモードを切り替えます。
  // Ctrl+q+f-カーソルの下のテーブルをフォーマットします。(検索とたまにかち合うのでctrl+q 後 fにするかも)
  {
    "key": "ctrl+q f",
    "command": "text-tables.formatUnderCursor",
    "when": "tableMode"
  },
  // Ctrl+q 後 スペース-カーソルのセルをクリアします。
  // ※ ctrl + space で 過去の入力履歴がでる
  // ctrl+q 後 tテーブル新規作成
  {
    "key": "ctrl+q t",
    "command": "text-tables.createTable",
    "when": "tableMode"
  },
  // tabで次のセルに
  {
    "key": "tab",
    "command": "text-tables.gotoNextCell",
    "when": "tableMode"
  },
  // shift+tabで前のセルに
  {
    "key": "shift+tab",
    "command": "text-tables.gotoPreviousCell",
    "when": "tableMode"
  },
  // paste as table
  {
    "key": "ctrl+q p",
    "command": "markdownExtended.pasteAsTable",
    "when": "tableMode"
  },
]